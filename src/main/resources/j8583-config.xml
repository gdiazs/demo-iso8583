<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE j8583-config SYSTEM "http://j8583.sourceforge.net/j8583.dtd">
<j8583-config>

    <!-- Network response Message -->
    <template type="0810">
        <field num="7" type="DATE10"/>
        <field num="11" type="NUMERIC" length="6"/>
        <field num="37" type="ALPHA" length="12"/>
        <field num="39" type="ALPHA" length="2">00</field>
        <field num="70" type="NUMERIC" length="3"/>
    </template>

    <!-- Network request Message -->
    <parse type="0800">
        <field num="7" type="DATE10"/>
        <field num="11" type="NUMERIC" length="6"/>
        <field num="37" type="ALPHA" length="12"/>
        <field num="70" type="NUMERIC" length="3"/>
    </parse>
</j8583-config>
